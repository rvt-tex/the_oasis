
<%= form_for :client do |f| %>

<br><br>
<%= f.label :first_name %>
<%= f.text_field :first_name, required:true %>

<br><br>
<%= f.label :last_name %>
<%= f.text_field :last_name, required:true %>


<br><br>
<%= f.label :phone %>
<%= f.number_field :phone %> 

<br><br>
<%= f.label :email %>
<%= f.email_field :email %>

<br><br>
<%= f.label :password %>
<%= f.password_field :password %>

<br><br>
<%= f.submit 'Register' %>
<% end %>